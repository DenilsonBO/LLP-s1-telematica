#include <stdio.h>

int main()
{
    int matrizA[3][3] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
    int matrizB[3][3] = {{9, 8, 7}, {6, 5, 4}, {3, 2, 1}};
    int matrizSoma[3][3];
    int i, j;

    printf("Matriz A:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            printf("%d ", matrizA[i][j]);
        }
        printf("\n");
    }

    printf("Matriz B:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            printf("%d ", matrizB[i][j]);
        }
        printf("\n");
    }

    printf("Matriz Soma:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            matrizSoma[i][j] = matrizA[i][j] + matrizB[i][j];
            printf("%d ", matrizSoma[i][j]);
        }
        printf("\n");
    }

    return 0;
}
